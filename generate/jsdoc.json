{
    "opts": {
        "template": "node_modules/docdash"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "docdash": {
        "static": true,
        "sort": true,
        "search": true,
        "collapse": true,
        "typedefs": true,
        "removeQuotes": "none",
        "scripts": [],
        "menu":{
            "Github repo": {
                "href":"https://github.com/cosinekitty/astronomy",
                "target":"_blank",
                "class":"menu-item",
                "id":"repository"
            }
        }
    },
    "templates": {
        "default": {
            "useLongnameInNav": true
        }
    }
}